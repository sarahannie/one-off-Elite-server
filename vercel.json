{
    "version": 2,
    "builds": [
      {
        "src": "index.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/index.js"
      }
    ],
    "env": {
      "EMAIL_USER": "@email_user",
      "EMAIL_PASS": "@email_pass"
    }
  }
  